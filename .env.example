# Pooled connection — used at runtime (Next.js / Prisma queries)
DATABASE_URL=postgresql://user:pass@ep-xxx-pooler.region.aws.neon.tech/neondb?sslmode=require

# Direct connection — used by Prisma migrations (no pgBouncer)
DIRECT_URL=postgresql://user:pass@ep-xxx.region.aws.neon.tech/neondb?sslmode=require

# Admin password for /login
ADMIN_PASSWORD=your-admin-password

# Random 32-char secret for iron-session cookie signing
SESSION_SECRET=your-32-char-random-secret-string
